{
    "name": "nacha-cheese",
    "version": "1.0.1",
    "description": "Nacha Cheese is a TypeScript library for generating NACHA (Automated Clearing House) files, which are used to initiate ACH transactions in the United States.",
    "main": "dist/nacha.js",
    "types": "dist/nacha.d.ts",
    "scripts": {
        "build": "tsc",
        "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec",
        "prepublish": "pnpm build"
    },
    "type": "module",
    "keywords": [
        "nacha",
        "ach",
        "automated clearing house"
    ],
    "author": "Lunch Payments",
    "repository": {
        "type": "git",
        "url": "https://github.com/lunchpayments/nacha-cheese"
    },
    "license": "MIT",
    "dependencies": {
        "date-fns": "^3.6.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/chai": "^4.3.17",
        "@types/mocha": "^10.0.7",
        "@types/node": "^22.3.0",
        "chai": "^5.1.1",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "mocha": "^10.7.3",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.1.0"
    }
}
